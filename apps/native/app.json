{
  "expo": {
    "owner": "van.tran",
    "name": "Grocery",
    "slug": "grocery-app",
    "version": "1.0.0",
    "scheme": "com.turbo.example",
    "orientation": "portrait",
    "icon": "./assets/icons/icon-light.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c4f8ebdb-01f9-49be-ace5-d477daff4e29"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.turbo.example",
      "icon": {
        "light": "./assets/icons/icon-light.png",
        "dark": "./assets/icons/icon-dark.png",
        "tinted": "./assets/icons/icon-tinted.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.turbo.example"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/icons/splash-icon.png",
          "dark": {
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 25
          }
        }
      ],
      "@logrocket/react-native",
      "expo-secure-store",
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c4f8ebdb-01f9-49be-ace5-d477daff4e29"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
